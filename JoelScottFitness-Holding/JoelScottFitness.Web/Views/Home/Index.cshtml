    @Styles.Render("~/Content/css")

</head>
<body>



    <style>
        body {
            background-color: black;
        }

        .title-container {
            text-align: center;
            width: 100%;
        }

        .title {
            max-width: 100%;
            width: auto;
            height: auto;
            padding-left: 20px;
            padding-right: 20px;
        }


        .coming-soon {
            padding-top: 70px;
            padding-bottom: 70px;
            font-size: 30pt;
            color: white;
            font-family: "Graduate-Regular", Helvetica, Arial, sans-serif;
            text-align: center;
        }

        #footer {
            background-color: black;
            height: 500px;
        }

        #footer-links {
            padding-top: 30px;
            padding-bottom: 50px;
        }

        #footer-links ul {
            list-style-type: none;
            text-align: center;
        }

        #footer-links ul li {
            display: inline;
        }

        #footer-links ul li:hover {
            color: white;
        }

        #footer-links ul a {
            color: lightgray;
        }

        #footer-links ul a:hover {
            color: white;
        }

        #footer-social {
            color: white;
        }

        .footer-icons {
            text-align: center;
            padding-bottom: 30px;
        }

        .footer-icons i {
            color: #999999;
        }

        .footer-icons i:hover {
            color: white;
        }

        .footer-logo {
            width: 100%;
            padding-top: 30px;
            padding-bottom: 30px;
        }

        .footer-logo-wrapper {
            position: absolute;
            left: 50%;
            transform: translateX(-50%);
            white-space: nowrap;
        }

        .footer-links-container {
            padding-top: 50px;
        }

        .footer-links-container ul {
            list-style: none;
        }

        .footer-links-container ul li {
            padding-bottom: 20px;
            padding-bottom: 20px;
        }

        .footer-links-container ul li a {
            text-decoration: none;
            color: #999999;
            padding-bottom: 10px;
        }

        .footer-links-container ul li a:hover {
            color: white;
        }
